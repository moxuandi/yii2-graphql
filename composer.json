{
	"name": "moxuandi/yii2-graphql",
	"description": "facebook graphql server side for yii2 php framework",
	"keywords": ["yii2", "graphql"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "QingShan li",
		"email": "21997272@qq.com"
	}, {
		"name": "zhangmoxuan",
		"email": "1104984259@qq.com"
	}],
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.2.0",
		"yiisoft/yii2": "~2.0.35",
		"webonyx/graphql-php": "^14.3",
		"ecodev/graphql-upload": "^6.0.0",
		"laminas/laminas-diactoros": "^2.4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "4.8.*"
	},
	"autoload": {
		"psr-4": {
			"yii\\graphql\\": "src"
		}
	},
	"archive": {
		"exclude": [
			"/tests"
		]
	}
}
